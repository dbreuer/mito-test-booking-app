{
  "name": "mito-test-booking-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "changelog": "auto-changelog --template changelog-template.hbs -p -u --commit-limit false --output ./CHANGELOG.md"
  },
  "dependencies": {
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.35.1"
  },
  "devDependencies": {
    "@types/react": "17.0.13",
    "auto-changelog": "^2.3.0",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.1",
    "path": "^0.12.7",
    "typescript": "4.3.5"
  },
  "auto-changelog": {
    "commitLimit": false,
    "unreleased": true,
    "issueUrl": "https://app.clickup.com/t/{id}",
    "replaceText": {
      "[Ff]eature:": "",
      "[Ff]ix:": "",
      "[Bb]reak:": "",
      "^ #(.{6})\\s": "[$1](https://app.clickup.com/t/$1) - "
    },
    "includeBranch": [
      "develop",
      "master"
    ]
  }
}
